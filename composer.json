{
  "version": "0.0.1",
  "name": "garvinhicking/gh-validationdummy",
  "type": "typo3-cms-extension",
  "description": "Validation example",
  "authors": [
    {
      "name": "Garvin Hicking",
      "homepage": "https://garv.in",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">= 8.1.0, <= 8.3.99",
    "typo3/cms-core": "^12.4 || ^13.0"
  },
  "autoload": {
    "psr-4": {
      "GarvinHicking\\ValidationDummy\\": "Classes"
    }
  },
  "replace": {
    "garvinhicking/gh_validationdummy": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "gh_validationdummy"
    }
  }
}  
